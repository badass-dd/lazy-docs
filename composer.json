{
  "name": "badass-dd/lazy-docs",
  "description": "Intelligent PHPDoc generator for Laravel API controllers with natural language support",
  "type": "library",
  "homepage": "https://github.com/badass-dd/lazy-docs",
  "license": "MIT",
  "authors": [
    {
      "name": "Axelba",
      "email": "axelba@gmaill.com"
    },
    {
      "name": "Assurda77",
      "email": "assurda.77@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/console": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Badass\\ControllerPhpDocGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Badass\\ControllerPhpDocGenerator\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Badass\\ControllerPhpDocGenerator\\ControllerPhpDocGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
